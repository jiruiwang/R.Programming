makeCacheMatrix <- function(x = matrix()) {
    inv <- NULL
    set <- function(y) {
        x <<- y
        inv <<- NULL
    }
    get <- function() x
    setinv <- function(inv) inv <<- inv
    getinv <- function() inv
    list(set=set, get=get, setinverse=setinv, getinverse=getinv)
}

cacheSolve <- function(x) {
    m <- x$getImatrix()
    if(!is.null(m)){
        message("getting cached data")
        return(m)
    }
        data <- x$get()
        m <- solve(data)
        x$setinv(m)
        return(m)
}
